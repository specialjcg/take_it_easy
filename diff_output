+        lastActionTime = 0;
+        currentMyTurn = false;
+        addDebugLog('üîÑ √âtat polling reset');
+    };
+
+    const forceRefresh = async () => {
+        const currentSession = session();
+        if (currentSession) {
+            markActionPerformed();
+            await pollGameplayState(currentSession.sessionId);
+            addDebugLog('üîÑ Refresh forc√©');
+        }
+    };
+
+    // ============================================================================
+    // NETTOYAGE
+    // ============================================================================
+
     onCleanup(() => {
         stopPolling();
+        addDebugLog('üßπ usePolling cleanup');
     });
 
+    // ============================================================================
+    // API PUBLIQUE SIMPLIFI√âE
+    // ============================================================================
