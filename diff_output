+        current_tile_image: String::new(), // ‚úÖ Cha√Æne vide (pas d'image)
+        current_turn: 0,
+        waiting_for_players: vec![],
+        is_game_finished: false,
+        final_scores: String::new(),
+        error: Some(Error {
+            code: "GET_GAME_STATE_FAILED".to_string(),
+            message,
+            details: std::collections::HashMap::new(),
+        }),
+    }
